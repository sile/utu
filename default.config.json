{
  "keybindings": {
    "__global__": {
      "C-c": "quit",
      "C-g": "cancel",
      "C-h": "legend"
    },
    "__main__": {
      "C-s": "save",
      "C-u": "undo",
      "C-b": "bg(_)",
      "↑,i": "prev-line",
      "↓,k": "next-line",
      "←,j": "prev-char",
      "→,l": "next-char",
      "d": "dot(o)",
      " ": "dot( )",
      "m": "scope(marker)"
    },
    "marker": {
      "s": "mark-stroke",
      "l": "mark-line",
      "r": "mark-rect",
      "f": "mark-fill"
    }
  }
}
